{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "standard-with-typescript", "plugin:prettier/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["tsconfig.json"]
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["react"],
    "rules": {
      "multiline-ternary": "off"
    }
}
